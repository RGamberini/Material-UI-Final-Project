<VBox fx:id="cardVBox" prefHeight="385.0" prefWidth="385.0" styleClass="card" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <children>
            <VBox fx:id="headerVbox" prefHeight="160.0" prefWidth="178.0" spacing="10.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="16.0" />
               </HBox.margin>
            </VBox>
            <StackPane prefHeight="160.0">
               <children>
                  <ImageView fx:id="profileImage" fitHeight="160.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true">
                     <StackPane.margin>
                        <Insets />
                     </StackPane.margin>
                     <image>
                        <Image url="@../resources/avatars/Person/user_male_banned_material.png" />
                     </image></ImageView>
                  <Circle fill="#1f93ff00" radius="75.0" stroke="#00000082" strokeType="INSIDE" strokeWidth="2.0">
                     <effect>
                        <DropShadow />
                     </effect>
                  </Circle>
               </children>
               <HBox.margin>
                  <Insets top="-12.0" />
               </HBox.margin>
            </StackPane>
         </children>
         <VBox.margin>
            <Insets left="-16.0" right="-16.0" />
         </VBox.margin>
      </HBox>
      <JFXListView fx:id="profileCardList">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </JFXListView>
      <HBox alignment="CENTER_RIGHT" style="-fx-border-style: solid hidden hidden hidden; -fx-border-color: #FF00FF00;">
         <children>
            <JFXButton alignment="CENTER_RIGHT" contentDisplay="BOTTOM" text="Submit" />
            <JFXButton alignment="CENTER_RIGHT" contentDisplay="BOTTOM" text="Submit">
               <HBox.margin>
                  <Insets right="16.0" />
               </HBox.margin></JFXButton>
         </children>
         <VBox.margin>
            <Insets left="-16.0" right="-16.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
