                        <Insets />
                     </StackPane.margin>
                     <image>
                        <Image url="@../resources/avatars/Person/user_male_banned_material.png" />
                     </image></ImageView>
                  <Circle fill="#1f93ff00" radius="75.0" stroke="#00000082" strokeType="INSIDE" strokeWidth="2.0">
                     <effect>
                        <DropShadow />
                     </effect>
                  </Circle>
               </children>
               <HBox.margin>
                  <Insets top="-12.0" />
               </HBox.margin>
            </StackPane>
         </children>
         <VBox.margin>
            <Insets left="-16.0" right="-16.0" />
         </VBox.margin>
      </HBox>
      <JFXListView fx:id="profileCardList">
