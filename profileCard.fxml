<VBox prefHeight="385.0" prefWidth="385.0" styleClass="card" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ProfileCardController">
   <children>
      <HBox>
         <children>
            <VBox fx:id="headerVbox" prefHeight="124.0" prefWidth="178.0" spacing="10.0" HBox.hgrow="ALWAYS">
